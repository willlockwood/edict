<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:bottom="-3dp"
        android:left="-3dp"
        android:right="-3dp"
        android:top="1dp">
        <shape android:shape="rectangle" >
            <stroke
                android:dashGap="5dp"
                android:dashWidth="5dp"
                android:width="1dp"
                android:color="@color/offWhite" />
        </shape>
    </item>
</layer-list>

